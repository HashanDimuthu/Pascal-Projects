program maltiply(input,output);

var     no1:integer;
        no2:integer;
        maltiply:integer;

begin   writeln('Enter Your 1 st Number');
	readln(no1);

	writeln('Enter Your 2 nd Number');
	readln(no2);

        matiply:=no1*no2;

	writeln('Your Maltiply is',' ',maltiply);
	readln;
end.
